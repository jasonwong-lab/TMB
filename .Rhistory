library("survival")
library("survminer")
#  plot survival
data<-read.table("~/Projects/tmb/survival_analysis/ng_paper/model/v2/clinical_info.txt",header = T,row.names = 1,sep="\t")
blca<-subset(data,Cancer.Type=="LUAD")
blca$group<-rep(NA,nrow(blca))
blca$group[which(blca$obs_tmb>=10,)]<-"g1"
blca$group[which(blca$obs_tmb<10,)]<-"g2"
survival.data<-subset(blca,select = c("SURVIVAL_EVENT","SURVIVAL_MONTHS","group"))
colnames(survival.data)<-c("os","time","group")
survival.data$group<-factor(survival.data$group,c("g1","g2"))
survival.data<-subset(survival.data,time<=36)
